[access "refs/heads/*"]
        label-Code-Review = -2..+2 group tooz-core
        label-Approved = +0..+1 group tooz-core
        workInProgress = group tooz-core
[access "refs/tags/*"]
        create = group tooz-core
        pushTag = group tooz-core
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
